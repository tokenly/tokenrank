{
    "name": "tokenly/tokenrank",
    "description": "A viewer for available Swapbot swaps",
    "keywords": ["tokenly","swapbot"],
    "license": "AGPL-3.0",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "guzzlehttp/guzzle": "~5",
        "tokenly/bitcoin-currency-lib": "dev-master",
        "tokenly/swapbot-client": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
